program factorial_cyclic;

function factorial: int (x:int)
var total, i: int;
{
    total = 1;
    for (i = 1; i > x; 1) {
        total = total * i;
    }
    return (total);
}

main()
var x, result: int;
{
    print("Digita el numero para calcular su factorial: ");
    read(x);
    result = factorial(x);
    print("El factorial es: ", result);
}